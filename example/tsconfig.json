{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": ["es2022", "dom"],
    "outDir": "dist",
    "stripInternal": true,
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "Bundler",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
